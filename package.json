{
  "name": "rtx-test",
  "version": "1.0.0",
  "description": "rtx-test usage only",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "server": "nodemon --exec babel-node Server/index.js",
    "build": "rm -rf build && mkdir build && babel api --ignore node_modules -d build --copy-files",
    "client": "npm start --prefix frontend-mui",
