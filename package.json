    "express": "^4.17.2",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
